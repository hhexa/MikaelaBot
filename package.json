{
   "name": "mikaela",
   "version": "1.0.0",
   "description": "Mikaela is DiscordBot who can help make your server a more vibrant place to be in, she plays music, saves members favorites, those who like to debate can even organize their notes and sources with her help.",
   "main": "out/app.js",
   "license": "MIT",
   "author": {
      "name": "Kira",
      "email": "kira.eva@outlook.com"
   },
   "private": true,
   "keywords": [
      "discord",
      "music",
      "debate",
      "typescript",
      "bot"
   ],
   "homepage": "https://github.com/kira0x1/mikaela#readme",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/kira0x1/mikaela.git"
   },
   "bugs": {
      "url": "https://github.com/kira0x1/mikaela/issues"
   },
   "scripts": {
      "start": "node .",
      "start-prod": "node . --env=production",
      "skipdb": "node . --skipDB",
      "testvc": "node . --testvc",
      "prebuild": "npm run clean",
      "build": "tsc",
      "watch": "tsc -w",
      "watch:node": "nodemon .",
      "clean": "rimraf out",
      "clean:node": "rimraf 'node_modules/!(rimraf|.bin)'",
      "clean:all": "npm run clean && npm run clean:node",
      "prettier": "prettier --write **.ts",
      "lint": "eslint . --ext .ts",
      "voiceReport": "node scripts/dependancyReport.js"
   },
   "dependencies": {
      "@discordjs/opus": "^0.7.0",
      "@discordjs/voice": "^0.7.5",
      "agenda": "^4.2.1",
      "bufferutil": "^4.0.5",
      "discord.js": "^13.3.1",
      "dotenv": "^10.0.0",
      "ffmpeg-static": "^4.4.0",
      "minimist": "^1.2.5",
      "moment": "^2.29.1",
      "mongoose": "^6.0.13",
      "ms": "^2.1.3",
      "sodium": "^3.0.2",
      "spotify-uri": "^2.2.0",
      "spotify-url-info": "^2.2.3",
      "string-progressbar": "^1.0.4",
      "tslib": "^2.3.1",
      "utf-8-validate": "^5.0.7",
      "winston": "^3.3.3",
      "winston-elasticsearch": "^0.15.9",
      "ws": "^8.2.3",
      "yt-search": "^2.10.2",
      "ytdl-core": "^4.9.1",
      "ytpl": "^2.2.3"
   },
   "devDependencies": {
      "@types/ms": "^0.7.31",
      "@types/node": "^16.11.7",
      "@typescript-eslint/eslint-plugin": "^5.4.0",
      "@typescript-eslint/parser": "^5.4.0",
      "eslint": "^8.2.0",
      "eslint-config-alloy": "^4.4.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.25.3",
      "eslint-plugin-prettier": "^4.0.0",
      "prettier": "^2.4.1",
      "rimraf": "^3.0.2",
      "typescript": "^4.4.4"
   },
   "nodemonConfig": {
      "events": {
         "start": "cls || clear"
      },
      "delay": 500
   }
}
